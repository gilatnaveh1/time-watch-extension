{
  "name": "time-Watch",
  "version": "0.1",
  "manifest_version": 2,
  "description": "fill timewatch",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": ["start.js"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Time Watch",
    "default_icon": "assets/images/icon128.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls>"
  ],
  "icons": {
    "16": "assets/images/icon16.png",
    "32": "assets/images/icon32.png",
    "48": "assets/images/icon48.png",
    "128": "assets/images/icon128.png"
  }
}